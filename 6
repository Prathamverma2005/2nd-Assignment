#include <iostream>
using namespace std;

class Counter {
private:
    static int count; 

public:
    static void increment() {
        count++;
        cout << "Current count: " << count << endl;
    }
};

int Counter::count = 0;

int main() {
    Counter::increment();
    Counter::increment();
    Counter obj;
    obj.increment();

    return 0;
}
